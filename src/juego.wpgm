import wollok.game.*
import pepita.*
import posiciones.*
import habitacion.*


program juego {

	const nivel1 = new Habitacion(puertas= new Puerta(position= game.at(3,2)))
	const nivel2 = new Habitacion(puertas= new Puerta(position= game.at(2,2)))
	
	
	const habitacionesAJugar = [nivel1, nivel2]
	habitacionManager.habitaciones(habitacionesAJugar)
	habitacionManager.cargarHabitacion(0)

	game.start()
}