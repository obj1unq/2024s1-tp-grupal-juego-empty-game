import wollok.game.*
import pepita.*
import posiciones.*
import habitacion.*


program juego {
	
	const nivel1 = new Habitacion(numeroHabitacion = 1)
	const nivel2 = new Habitacion(numeroHabitacion = 2)
	const puerta = new Puerta(siguienteHabitacion = nivel2);
	nivel1.agregarPuerta(puerta)
	
	habitacionManager.cargarHabitacion(nivel1)

	game.start()
}