import wollok.game.*
import menu.*

program juego {
	game.title("Space Defender")
	game.width(25) //previo 31 
	game.height(14) //previo 19
	
	
	//CONFIGURAR MENU
	const fondo = game.sound("musicaFondo.mp3")
	fondo.shouldLoop(true)
	game.schedule(500, { fondo.play()} )
	menu.iniciar()
	
	//INICIO 
		game.start()

}